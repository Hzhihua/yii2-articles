{
    "name": "hzhihua/yii2-articles",
    "description": "yii2-articles module yii2 articles",
    "type": "module",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "cnzhihua",
            "email": "cnzhihua@gmail.com",
            "github": "https://github.com/Hzhihua/yii2-articles"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.6",
        "hzhihua/yii2-dump": "*",
        "hzhihua/yii2-frappe-charts": "dev-master",
        "yii2mod/yii2-bootstrap-notify": "*",
        "yiier/yii2-editor.md": "*",
        "2amigos/yii2-selectize-widget": "~1.0",
        "2amigos/yii2-ckeditor-widget": "~2.1",
        "trntv/yii2-datetime-widget": "*",
        "kartik-v/yii2-grid": "@dev",
        "hzhihua/yii2-bootstrap-fileinput": "dev-master"
    },
    "config": {
        "process-timeout": 1800,
        "github-protocols": ["https", "http"]
    },
    "autoload": {
        "psr-4": {
            "hzhihua\\articles\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
